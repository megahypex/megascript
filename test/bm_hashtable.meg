class Zoo:
  func _init():
    self.aardvark = 1
    self.baboon   = 1
    self.cat      = 1
    self.donkey   = 1
    self.elephant = 1
    self.fox      = 1
  end 

  func ant(): return self.aardvark end 
  func banana(): return self.baboon end
  func tuna(): return self.cat end
  func hay(): return self.donkey end
  func grass(): return self.elephant end
  func mouse(): return self.fox end
end

var zoo = Zoo()
var sum = 0
var start = clock()
while sum < 100000000: 
  sum = sum + zoo.ant()
            + zoo.banana()
            + zoo.tuna()
            + zoo.hay()
            + zoo.grass()
            + zoo.mouse()
end

print(clock() - start)
print(sum)
